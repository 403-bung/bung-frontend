{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "api": ["./api.ts"],
      "components/*": ["components/*"],
      "img/*": ["img/*"],
      "icons/*": ["icons/*"],
      "hooks/*": ["hooks/*"],
      "pages/*": ["pages/*"],
      "Routes/*": ["Routes/*"],
      "store/*": ["store/*"],
      "utils/*": ["utils/*"]
    }
  },
  "include": [
    "src",
    "bung-frontend/src/Routes",
    "bung-frontend/src/App.tsx",
    "bung-frontend/src/index.tsx",
    "bung-frontend/src/pages"
  ]
}
